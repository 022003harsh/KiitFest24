generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model User {
    id                      String  @id @default(uuid())
    name                    String
    kiitEmail               String  @unique
    personalEmail           String  @unique
    phoneNumber             String?
    password                String
}